void setup() {
  Serial.begin(115200);
pinMode(2,OUTPUT);
  // Initialize OLED
  if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for (;;);
  }

  display.clearDisplay();
  display.display();

  // Initialize WiFi
  WiFi.begin(wifi_name, wifi_password);
  Serial.print("Connecting to WiFi");
  while (WiFi.status() != WL_CONNECTED) {
    delay(500);
    Serial.print(".");
     display.clearDisplay();
  display.setTextSize(2);
  display.setTextColor(SSD1306_WHITE);
  display.setCursor(0, 20);
  display.print(".");
  display.display();
  }
  Serial.println("\nConnected to WiFi");
  
  // Initialize Firebase
  Firebase.begin(firebase_url, firebase_secretkey);
    display.clearDisplay();
  display.setTextSize(2);
  display.setTextColor(SSD1306_WHITE);
  display.setCursor(0, 20);
  display.print("CONNECTED TO CLOUD");
  display.display();
}

void loop() {
  // Fetch data from Firebase
  if (Firebase.get(firebasedata, "/sensor/state")) {
    data = firebasedata.intData();;
    
    // Show image based on value
    display.clearDisplay();
    
    if (data == 0) {
      // Display Dog Image
      display.drawBitmap(0, 0, dogBitmap, 128, 64, WHITE);
      digitalWrite(2,LOW);
    } else if (data == 1) {
      // Display Cat Image
      display.drawBitmap(0, 0, DoorbellBitmap, 128, 64, WHITE);
      digitalWrite(2,HIGH);
    }
     else if (data == 2) {
      // Display Cat Image
      display.drawBitmap(0, 0, DooropenBitmap, 128, 64, WHITE);
      digitalWrite(2,HIGH);
     }
    else if (data == 3) {
      // Display Cat Image
      display.drawBitmap(0, 0, SmokeBitmap, 128, 64, WHITE);
      digitalWrite(2,HIGH);
    }
    display.display();
  } else {
    Serial.println("Failed to get data from Firebase");
  }
  delay(1000); // Delay for 1 second
}
